{
  "name": "sprosivracha/sdk-esb",
  "description": "SDK for RabbitMQ interactions in PHP 7.1 +",
  "type": "library",
  "version": "0.0.1",
  "require": {
    "php": ">=7.1",
    "php-amqplib/php-amqplib": "3.5.4",
    "psr/log": "^1.1 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "SdkEsb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SdkEsb\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^9.5"
  }
}
